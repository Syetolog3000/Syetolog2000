#include<iostream>

using namespace std;

int main() {
	int a, b, c;
	setlocale(LC_ALL, "Russian");
	cout << "Введите пол(Юноша-1, Девушка-2)\n";
	cin >> c;

	if (c == 1) {
		cout << "Введите сумму Юноши\n";
		cin >> a;


		if (a >= 75)
			cout << "Превосходная\n";
		if ((a > 50)&& (a < 75))
				cout << "Отличная\n";
		if ((a > 31)&& (a < 51))
				cout << "Хорошая\n";
		if ((a > 20)&& (a < 32))
				cout << "Удовлетврорительная\n";
		if ((a > 9)&& (a < 21))
				cout << "Плохая\n";
		if (a < 10)
			cout << "Очень плохая\n";
	}
	else if (c == 2) {
		cout << "Введите сумму Девушки\n";
		cin >> b;


		if (b >= 65)
			cout << "Превосходная\n";
		if ((b > 40)&&(b < 65))
				cout << "Отличная\n";
		if ((b > 26)&&(b < 41))
				cout << "Хорошая\n";
		if ((b > 15)&&(b < 27))
				cout << "Удовлетврорительная\n";
		if ((b > 7)&&(b < 16))
				cout << "Плохая\n";
		if (b < 8)
			cout << "Очень плохая\n";
	}
	return 0;
	system("pause");
}
